{
  "name": "files_push",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "main": "electron/main-simple.js",
  "author": "文件推送工具团队",
  "description": "Windows平台文件推送工具，支持拖拽文件到手机",
  "scripts": {
    "dev": "concurrently \"vite\" \"electron .\"",
    "build": "node ./node_modules/vite/bin/vite.js build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc -b --noEmit",
    "electron": "electron .",
    "electron:dev": "electron .",
    "clean:dist": "rimraf dist",
    "predist": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"Get-Process | Where-Object { $_.Path -and $_.Path -like 'F:\\\\ProgramPlace\\\\Company\\\\UC\\\\file-tool\\\\files_push\\\\dist\\\\win-unpacked\\\\*' } | Stop-Process -Force; Get-Process -Name 'electron' -ErrorAction SilentlyContinue | Stop-Process -Force; Start-Sleep -Seconds 1; try { Remove-Item -Recurse -Force 'F:\\\\ProgramPlace\\\\Company\\\\UC\\\\file-tool\\\\files_push\\\\dist' -ErrorAction Stop } catch { } ; exit 0\" && npm run build",
    "dist": "cross-env ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/ ELECTRON_BUILDER_BINARIES_MIRROR=https://npmmirror.com/mirrors/electron-builder-binaries/ electron-builder"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "electron-store": "^10.0.0",
    "lucide-react": "^0.511.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "tailwind-merge": "^3.0.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.7",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "cross-env": "^7.0.3"
  }
}